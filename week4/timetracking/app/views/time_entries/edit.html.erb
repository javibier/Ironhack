<h1>Editing time entries</h1>

<%= form_for [@project, @time_entry] do |f| %>
 	<%= f.label :hours %>
 	<%= f.text_field :hours %>
    <br>
 	<%= f.label :minutes %>
 	<%= f.text_field :minutes %>
    <br>
 	<%= f.label :date %>
 	<%= f.text_field :date %>
    <br> 
    <%= f.label :comment %>
 	<%= f.text_field :comment%>
    <br> 
 	<%= f.button "Save"%>
  <%end%>
